{
  "canisters": {
    "internet_identity": {
      "type": "custom",
      "candid": "src/internet_identity/internet_identity.did",
      "wasm": "internet_identity.wasm",
      "build": "curl -sSL https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm -o internet_identity.wasm"
    },
    "whoami": {
      "main": "src/whoami/main.mo",
      "type": "motoko"
    },
    "identity_manager": {
      "type": "custom",
      "candid": "src/identity_manager/identity_manager.did",
      "wasm": "src/identity_manager/identity_manager.wasm"
    },
    "pub_sub_channel": {
      "type": "custom",
      "candid": "src/pub_sub_channel/pub_sub_channel.did",
      "wasm": "src/pub_sub_channel/pub_sub_channel.wasm"
    },
    "auth_client_demo_assets": {
      "dependencies": ["whoami"],
      "frontend": {
        "entrypoint": "src/auth_client_demo_assets/src/index.html"
      },
      "source": [
        "src/auth_client_demo_assets/assets",
        "dist/auth_client_demo_assets/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
